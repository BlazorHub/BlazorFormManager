@if (Result != null)
{
<div class="blazor-form-manager-message">
    <p class="alert alert-@(Result.Succeeded ? "success" : "danger")">@Result.Message</p>
</div>
}

@code {
    /// <summary>
    /// Gets or sets the form submission result.
    /// </summary>
    [Parameter] public FormManagerSubmitResult Result { get; set; }
}
